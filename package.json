{
  "name": "@notifycode/hash-it",
  "version": "0.0.6",
  "author": "Neza",
  "main": "./dist/index.js",
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.2.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "keywords": ["bun", "crypto", "token", "aes", "typescript"],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir=dist --minify --target=node --format=esm --outfile=dist/index.js",
    "types": "tsc --project tsconfig.build.json",
    "prepare": "bun run build && bun run types"
  },
  "type": "module",
  "types": "./dist/index.d.ts"
}
